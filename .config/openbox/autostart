#!/usr/bin/sh

# Composite manager
xcompmgr -c &

# Wallpaper manager initialization
nitrogen --restore &

# Terminal initialization
urxvtd -q -o -f &
#sh -c ~/.bin/term_bg & #start script for transparent terminal as background

# Polybar at top of desktop start
[[ $(command -v polybar) ]] && sh ~/.bin/bar_start &
